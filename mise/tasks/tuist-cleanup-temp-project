#!/bin/bash
set -euo pipefail

# Read the temporary org name
if [ -f .tuist-temp-org ]; then
    ORG_NAME=$(cat .tuist-temp-org)
    echo "Deleting temporary organization: $ORG_NAME"
    TUIST_URL="https://canary.tuist.dev" tuist organization delete "$ORG_NAME" || echo "Failed to delete organization (may already be deleted)"
    rm -f .tuist-temp-org .tuist-temp-project
fi

# Restore original Tuist.swift
if [ -f Tuist.swift.original ]; then
    echo "Restoring original Tuist.swift"
    mv Tuist.swift.original Tuist.swift
fi
